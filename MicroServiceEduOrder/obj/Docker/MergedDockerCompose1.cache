services:
  microserviceeduorder:
    build:
      context: C:\udv\education\MicroServiceEduOrder
      dockerfile: MicroServiceEduOrder/Dockerfile
      labels:
        com.microsoft.created-by: visual-studio
        com.microsoft.visual-studio.project-name: MicroServiceEduOrder
      target: base
    container_name: MicroServiceEduOrder
    entrypoint: tail -f /dev/null
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      ASPNETCORE_LOGGING__CONSOLE__DISABLECOLORS: "true"
      ASPNETCORE_URLS: https://+:443;http://+:80
      DOTNET_USE_POLLING_FILE_WATCHER: '1'
      NUGET_FALLBACK_PACKAGES: ''
    image: microserviceeduorder:dev
    labels:
      com.microsoft.visualstudio.debuggee.arguments: ' --additionalProbingPath /root/.nuget/packages  "/app/bin/Debug/net5.0/MicroServiceEduOrder.dll"'
      com.microsoft.visualstudio.debuggee.killprogram: /bin/sh -c "if PID=$$(pidof
        dotnet); then kill $$PID; fi"
      com.microsoft.visualstudio.debuggee.program: dotnet
      com.microsoft.visualstudio.debuggee.workingdirectory: /app
    ports:
    - target: 80
    - target: 443
    tty: true
    volumes:
    - C:\udv\education\MicroServiceEduOrder\MicroServiceEduOrder:/app:rw
    - C:\Users\rsc\vsdbg\vs2017u5:/remote_debugger:rw
    - C:\Users\rsc\AppData\Roaming\ASP.NET\Https:/root/.aspnet/https:ro
    - C:\Users\rsc\AppData\Roaming\Microsoft\UserSecrets:/root/.microsoft/usersecrets:ro
    - c:\udv\nuget_packages:/root/.nuget/packages:ro
    - C:\udv\education\MicroServiceEduOrder:/src:rw
version: '3.4'